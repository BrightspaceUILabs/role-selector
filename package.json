{
  "name": "@brightspace-ui-labs/role-selector",
  "description": "Select roles from a dialog with role items",
  "type": "module",
  "version": "3.0.0",
  "repository": "https://github.com/BrightspaceUILabs/role-selector.git",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "role-selector.js",
    "role-item.js",
    "role-selector.serge.json",
    "/lang",
    "/mixins"
  ],
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:style",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:style": "stylelint \"**/*.{js,html}\"",
    "start": "web-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "d2l-test-runner",
    "test:vdiff": "d2l-test-runner vdiff"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@brightspace-ui/stylelint-config": "^1",
    "@brightspace-ui/testing": "^1",
    "@web/dev-server": "^0.4",
    "eslint": "^8",
    "eslint-config-brightspace": "^1",
    "stylelint": "^16"
  },
  "dependencies": {
    "@brightspace-ui/core": "^3",
    "lit": "^3"
  }
}
